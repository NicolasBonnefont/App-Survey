{
  "expo": {
    "name": "Almanara-Survey",
    "slug": "Almanara-Survey",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#280000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "build": {
        "babel": {
          "include": [
            "galio-framework"
          ]
        }
      }
    },
    "description": "",
    "android": {
      "package": "com.nioox.AlmanaraSurvey"
    }
  }
}